{
    "name": "Debian",
    "dockerFile": "Dockerfile",
    "extensions": [
      "ms-python.python"
    ],
    "settings": {
      "python.pythonPath": "/usr/bin/python3.11"
    },
    "mounts": [
      "source=${localWorkspaceFolder}/app,target=/app,type=bind"
    ],
    "workspaceFolder": "/app",
    "containerEnv": {
        "LANG": "C.UTF-8"
    },
  }